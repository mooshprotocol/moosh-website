{
  "name": "moosh-website",
  "version": "0.1.0",
  "private": true,
  "description": "Modern, responsive website built with Next.js 15 and TypeScript",
  "keywords": [
    "nextjs",
    "typescript",
    "tailwind",
    "website",
    "react"
  ],
  "author": "Moosh Protocol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Moosh-Finance/moosh-website.git"
  },
  "bugs": {
    "url": "https://github.com/Moosh-Finance/moosh-website/issues"
  },
  "homepage": "https://github.com/Moosh-Finance/moosh-website#readme",
  "packageManager": "pnpm@10.12.2",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky install",
    "clean": "rm -rf .next node_modules/.cache"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "framer-motion": "^12.23.6",
    "lenis": "^1.3.8",
    "next": "15.4.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "14.0.0",
    "husky": "^9.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "lint-staged": "^15.0.0",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0"
  }
}